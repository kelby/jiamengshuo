<p id="notice"><%= notice %></p>

<h1>Listing Admin Catalogs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_catalogs.each do |admin_catalog| %>
      <tr>
        <td><%= admin_catalog.name %></td>
        <td><%= link_to 'Show', admin_catalog %></td>
        <td><%= link_to 'Edit', edit_admin_catalog_path(admin_catalog) %></td>
        <td><%= link_to 'Destroy', admin_catalog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Catalog', new_admin_catalog_path %>
